        "@patternfly/patternfly": "^4.224.2",
        "@sentry/browser": "^7.60.0",
        "@sentry/tracing": "^7.60.0",
        "@webcomponents/webcomponentsjs": "^2.8.0",
        "base64-js": "^1.5.1",
        "chart.js": "^4.3.1",
        "chartjs-adapter-moment": "^1.0.1",
        "codemirror": "^6.0.1",
